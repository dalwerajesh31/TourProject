package com.example.entity;

import java.util.Set;

import jakarta.persistence.CascadeType;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.OneToMany;
import jakarta.persistence.OneToOne;
import jakarta.persistence.Table;
@Entity
@Table(name="Customer_Master")
public class Customer_Master {
	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
    private int Cust_id;
	private String Cust_Name;
	private String Cust_details;
	@OneToMany(cascade = CascadeType.ALL )
	@JoinColumn(name = "Cust_id")
	private Set<Booking_Header> BookingHeader;
	public int getCust_id() {
		return Cust_id;
	}
	public void setCust_id(int cust_id) {
		Cust_id = cust_id;
	}
	public Set<Booking_Header> getBookingHeader() {
		return BookingHeader;
	}
	public void setBookingHeader(Set<Booking_Header> bookingHeader) {
		BookingHeader = bookingHeader;
	}
	public String getCust_Name() {
		return Cust_Name;
	}
	public void setCust_Name(String cust_Name) {
		Cust_Name = cust_Name;
	}
	public String getCust_details() {
		return Cust_details;
	}
	public void setCust_details(String cust_details) {
		Cust_details = cust_details;
	}

}
